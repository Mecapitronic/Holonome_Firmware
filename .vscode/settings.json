{
    "cSpell.words": [
        "Mecapi",
        "Mecapitronic",
        "otos",
        "setpoint",
        "teleplot",
        "uart",
        "wokwi"
    ],
    "files.associations": {
        "string": "cpp",
        "functional": "cpp",
        "queue": "cpp",
        "array": "cpp",
        "string_view": "cpp",
        "library_module.h": "c",
        "structure.h": "c",
        "move_module.h": "c",
        "deque": "cpp",
        "unordered_map": "cpp",
        "unordered_set": "cpp",
        "vector": "cpp",
        "initializer_list": "cpp",
        "cmath": "cpp",
        "*.tcc": "cpp",
        "rtc.h": "c",
        "esp_system.h": "c",
        "random": "cpp",
        "new": "cpp",
        "math_module.h": "c"
    },
    "launch": {
        "version": "0.2.0",
        "configurations": [
            {
                "name": "Wokwi GDB Esp32",
                "type": "cppdbg",
                "request": "launch",
                "program": ".pio/build/Esp32/firmware.elf",
                "cwd": "${workspaceFolder}",
                "MIMode": "gdb",
                "miDebuggerPath": "${userHome}/.platformio/packages/toolchain-xtensa-esp32/bin/xtensa-esp32-elf-gdb.exe",
                "miDebuggerServerAddress": "localhost:3333"
            }
        ]
    }
}